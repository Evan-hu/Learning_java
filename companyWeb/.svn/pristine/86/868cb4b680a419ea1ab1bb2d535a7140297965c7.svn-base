.userTop{padding: 10px; background: #F2EFE8; font-size: 20px; font-weight: bold; font-family: microsoft Yahei}
.crumb{ line-height: 40px;}

.userLeft{ float: left; width: 150px; background: #fff; border: 1px solid #d1d1d1;}
.userLeft h2{ height:29px; padding-left:35px; 
background-color: #d9d9d9;
  background-image: -webkit-gradient(linear, left top, left bottom, from(#f8f8f8), to(#d9d9d9)); /* Chrome, Safari 4+ */
  background-image: -webkit-linear-gradient(top, #f8f8f8, #d9d9d9); /* Chrome 10-25, iOS 5+, Safari 5.1+ */
  background-image:    -moz-linear-gradient(top, #f8f8f8, #d9d9d9); /* Firefox 3.6-15 */
  background-image:      -o-linear-gradient(top, #f8f8f8, #d9d9d9); /* Opera 11.10-12.00 */
  background-image:         linear-gradient(to bottom, #f8f8f8, #d9d9d9); /* Chrome 26, Firefox 16+, IE 10+, Opera 12.10+ */
 line-height: 28px; font-size: 14px;}

