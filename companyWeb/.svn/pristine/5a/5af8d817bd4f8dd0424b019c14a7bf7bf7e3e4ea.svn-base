.jcarousel-skin-tango .jcarousel-container-horizontal { width:280px;padding: 0 10px;}
.jcarousel-skin-tango .jcarousel-clip-horizontal {width:280px; height:64px;}
.jcarousel-skin-tango .jcarousel-item{width:64px; height:64px;}
.jcarousel-skin-tango .jcarousel-item img{ display:block; padding:1px; width:60px; height:60px; border:1px solid #ccc; cursor:pointer;}
.jcarousel-skin-tango .jcarousel-item img.on { padding:0;  border:2px solid #b20000; }
.jcarousel-skin-tango .jcarousel-item-horizontal { margin:0 3px;}
.jcarousel-skin-tango .jcarousel-item-placeholder {background: #fff; color: #000;}

.jcarousel-skin-tango .jcarousel-next-horizontal {position: absolute; top:25px; right:0px; width:7px; height:12px; cursor:pointer; background: transparent url(../images/slider_btn.gif) no-repeat -28px 0;}
.jcarousel-skin-tango .jcarousel-direction-rtl .jcarousel-next-horizontal{ left:0; right:auto; background-image: url(../images/slider_btn.gif);}
.jcarousel-skin-tango .jcarousel-next-horizontal:hover {background-position: -35px 0;}
.jcarousel-skin-tango .jcarousel-next-horizontal:active { background-position: -28px 0;}
.jcarousel-skin-tango .jcarousel-next-disabled-horizontal,
.jcarousel-skin-tango .jcarousel-next-disabled-horizontal:hover,
.jcarousel-skin-tango .jcarousel-next-disabled-horizontal:active { cursor: default; background-position: -21px 0;}
.jcarousel-skin-tango .jcarousel-prev-horizontal {position: absolute;top:25px; left:0; width:7px; height:12px; cursor:pointer; background: transparent url(../images/slider_btn.gif) no-repeat -7px 0;}
.jcarousel-skin-tango .jcarousel-direction-rtl .jcarousel-prev-horizontal { left:auto; right: 5px; background: url(../images/slider_btn.gif); }
.jcarousel-skin-tango .jcarousel-prev-horizontal:hover {background-position: -14px 0;}
.jcarousel-skin-tango .jcarousel-prev-horizontal:active { background-position: -7px 0;}
.jcarousel-skin-tango .jcarousel-prev-disabled-horizontal,
.jcarousel-skin-tango .jcarousel-prev-disabled-horizontal:hover,
.jcarousel-skin-tango .jcarousel-prev-disabled-horizontal:active {cursor: default; background-position: 0 0;}
