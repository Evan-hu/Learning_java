@charset "utf-8";
/* CSS Document */

#hd .pwrap .userinfo {
	width: 58%;
	display: inline-block;
	zoom: 1;
	letter-spacing: normal;
	word-spacing: normal;
	vertical-align: top;
	float: right;
}
#hd .pwrap .cclogo {
	width: 40%;
	display: inline-block;
	zoom: 1;
	letter-spacing: normal;
	word-spacing: normal;
	vertical-align: top;
 *float: left;
}
blockquote, ul, ol, dl, p, table, textarea {
	margin: 0;
	padding: 0;
}
ul {
	list-style: none;
}
ol {
	list-style: decimal inside;
}
.w960 {
	width: 960px;
	margin: 0 auto;
}
.w920 {
	width: 920px;
	margin: 0 auto;
}
.pwrap {
	width: 100%;
	margin: 0 auto;
	position: relative;
}
.clr {
	display: block;
	clear: both;
	margin: 0;
	padding: 0;
	height: 0px;
	line-height: 0px;
	font-size: 0px;
}
.inblock {
	display: inline-block;
	zoom: 1;
*display:inline;
}
.show {
	display: block;
}
.hide {
	display: none;
}
.text_tip {
	color: #8b8b8b;
}
.text_alert {
	color: #800000;
}
textarea {
	border: 1px solid #e0e0e0;
	padding: 5px;
}
table, table td, table th {
	border: none;
	padding: 0;
	margin: 0;
}
.link1 a, .link1 a:link, .link1 a:visited {
	color: #1f1f1f;
	text-decoration: none;
}
.link1 a:hover, .link1 a:active {
	color: #BA251D;
	text-decoration: underline;
}
.link2 a, .link2 a:link, .link2 a:visited {
	color: #BA251D;
	text-decoration: none;
}
.link2 a:hover, .link2 a:active {
	color: #BA251D;
	text-decoration: underline;
}
.link3 a, .link3 a:link, .link3 a:visited {
	color: #5c5c5c;
	text-decoration: none;
}
.link3 a:hover, .link3 a:active {
	color: #3c3b3b;
	text-decoration: underline;
}
hr {
	display: block;
	padding: 0;
margin 0;
	clear: both;
}
.hr1 {
	border: none;
	border-bottom: 1px dashed #D9D9D9;
}
.hr2 {
	border: none;
	border-bottom: 1px solid #D9D9D9;
}
input.text1 {
	height: 25px;
	line-height: 25px;
	margin: 0;
	padding: 0;
}
input.text2 {
	margin: 0;
	padding: 0.7em 5px;
	font-size: 13px;
	border: 1px solid #E4E4E4;
}
/*距离样式*/
.m15 {
	margin: 15px;
}
.mb15 {
	margin-bottom: 15px!important;
}
.mt15 {
	margin-top: 15px;
}
.m10 {
	margin: 10px;
}
.mb10 {
	margin-bottom: 10px;
}
.mt10 {
	margin-top: 10px;
}
.m20 {
	margin: 20px;
}
.mb20 {
	margin-bottom: 20px;
}
.mt20 {
	margin-top: 20px;
}
.p15 {
	padding: 15px;
}
.pb15 {
	padding-bottom: 15px;
}
/*字体*/
.f12 {
	font-size: 12px;
}
.f18 {
	font-size: 18px;
}
/*  header  */
#hd {
	position: relative;
	z-index: 1200;
}
.hd_bottom_border {
	border-bottom: 5px solid #f2f2f2;
}
.hd_bottom_border_inside {
	height: 5px;
	border-bottom: 1px solid #d0d0d0;
}
#default_index #hd .hd_bottom_border {
	border-bottom: 5px solid #FFF;
}
#default_index #hd .hd_bottom_border_inside {
	border-bottom: 1px solid #FFF;
}
#hd .logo {
	margin: 20px 0 0 0;
}
#hd .topbar {
	text-align: right;
	margin-top: 15px;
}
#hd .topbar2 {
	text-align: right;
	margin-top: 5px;
	font-size: 14px;
}
#hd .nav2 {
	font-size: 16px;
	font-family: "Microsoft Yahei", "SimHei";
	background-color: #F2F2F2;
	padding-top: 8px;
}
#hd .topbar ul, #hd .topbar ul li {
	display: inline-block;
	zoom: 1;
*display:inline;
}
#hd .topbar ul li {
	margin: 0 5px;
}
#hd .topbar ul li#loginbyweibo {
	cursor: pointer;
	background: url() no-repeat left center;
	padding-left: 20px;
}
#hd .topbar ul li.welcome {
	color: #777676;
}
#breadcrumbs {
	font-size: 14px;
	height: 38px;
	line-height: 38px;
	padding-left: 1em;
	margin-bottom: 20px;
	background: #f3f3f3;
	border: 1px solid #e2e2e2;
	border-bottom: 1px solid #cbcbcb;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	width: 190px;
	_width: 184px;
}
#breadcrumbs span {
	display: inline-block;
	zoom: 1;
*display:inline;
}
#breadcrumbs span.split {
	color: #6f6f6f;
}
#breadcrumbs span.cur {
	color: #6f6f6f;
	cursor: default;
}
/*  slide  */
#slide {
	background: url() repeat-x left top;
}
#slide .insbg {
	position: relative;
	height: 300px;
	background: url() no-repeat center top;
}
#silte .slide_item {
	position: relative;
}
/* main */
#bd {
	margin-top: 20px;
	position: relative;
	z-index: 50;
}
#bd #side {
	width: 198px;
	float: left;
}
#bd #main {
 *width: 220px;
	overflow: hidden;
	float: left;
	background-color: #FFF;
	padding-top: 10px;
	padding-bottom: 10px;
	padding-left: 20px;
}
#bd #mainlist {
	font-size: 12px !important;
	/*	margin-left: 220px;
	_margin-left: 110px;*/
	overflow: hidden;
	margin-bottom: 20px;
	margin-left: 30px;
}
.sharp_account #bd #main {
	overflow: visible;
}
#bd #direction h1 {
	font-family: "微软雅黑", "黑体";
	font-size: 26px;
	line-height: 32px;
	font-weight: normal;
}
#bd #side ul.catlist {
	list-style: none;
	margin: 0px;
	padding: 0px;
	border: 1px solid #d4d4d4;
	background: #FFF;
}
#bd #side ul.catlist li {
	height: 36px;
	line-height: 36px;
	border-top: #d4d4d4 solid 1px;
}
#bd #side ul.catlist li.l0 {
	font-size: 15px;
	border: none;
	font-weight: bold;
}
#bd #side ul.catlist li.l1 {
	font-size: 15px;
	font-weight: bold;
}
#bd #side ul.catlist li.l2 {
	font-size: 13px;
}
#bd #side ul.catlist li a {
	display: block;
	height: 36px;
}
#bd #side ul.catlist li.l0 a {
	padding-left: 1em;
}
#bd #side ul.catlist li.l1 a {
	padding-left: 1em;
}
#bd #side ul.catlist li.l2 a {
	padding-left: 2em;
}
#bd #side ul.catlist li.l0 a, #bd #side ul.catlist li.l0 a:link, #bd #side ul.catlist li.l0 a:visited {
	color: #1f1f1f;
	text-decoration: none;
	background: url() repeat-x bottom;
}
#bd #side ul.catlist li.l0 a:hover, #bd #side ul.catlist li.l0 a:active {
	color: #1f1f1f;
	text-decoration: underline;
}
#bd #side ul.catlist li.l1 a, #bd #side ul.catlist li.l1 a:link, #bd #side ul.catlist li.l1 a:visited, #bd #side ul.catlist li.l2 a, #bd #side ul.catlist li.l2 a:link, #bd #side ul.catlist li.l2 a:visited {
	color: #BA251D;
	text-decoration: none;
	background: url() no-repeat 170px center;
}
#bd #side ul.catlist li.l1 a:hover, #bd #side ul.catlist li.l1 a:active, #bd #side ul.catlist li.l2 a:hover, #bd #side ul.catlist li.l2 a:active {
	color: #FFF;
	text-decoration: underline;
	background: #BA251D url() no-repeat 170px center;
}
#bd #side ul.catlist li.cur a, #bd #side ul.catlist li.cur a:link, #bd #side ul.catlist li.cur a:hover, #bd #side ul.catlist li.cur a:active, #bd #side ul.catlist li.cur a:visited {
	color: #FFF;
	text-decoration: none;
	background: #BA251D url() no-repeat 170px center;
}
#bd #side ul.menu {
	list-style: none;
	margin: 0px;
	padding: 0px;
	border: 1px solid #d4d4d4;
	background: #FFF;
}
#bd #side ul.menu li {
	padding-right: 0;
	height: 37px;
	line-height: 36px;
	border-top: #d4d4d4 solid 1px;
}
#bd #side ul.menu li.l0 {
	font-size: 15px;
	color: #8b8b8b;
	text-align: center;
	background: url() repeat-x bottom;
	border: none;
}
#bd #side ul.menu li.l1 {
	font-size: 15px;
	color: #8b8b8b;
	padding-left: 26px;
}
#bd #side ul.menu li.l2 {
	font-size: 15px;
}
#bd #side ul.menu li a {
	display: block;
	padding-left: 50px;
}
#bd #side ul.menu li.l2 a, #bd #side ul.menu li.l2 a:visited, #bd #side ul.menu li.l2 a:link {
	color: #1f1f1f;
	text-decoration: none;
	background: url() no-repeat 170px center;
}
#bd #side ul.menu li.l2 a:hover {
	color: #BA251D;
	text-decoration: underline;
	background: #f4f4f4 url() no-repeat 170px center;
}
#bd #side ul.menu li.l2 a:active {
	color: #006699;
	text-decoration: underline;
	background: #f4f4f4 url() no-repeat 170px center;
}
/*  tab */
ul.tab_bar {
	font-size: 14px;
	z-index: 5;
	position: relative;
	background: #FFF;
	width: 200px;
	border-bottom-width: 1px;
	border-bottom-style: solid;
	border-bottom-color: #e2e2e2;
}
ul.tab_bar li {
	display: inline-block;
	zoom: 1;
*display:inline;
	width: auto;
	margin-left: 15px;
	background: none;
	border: none;
	margin-bottom: -1px;
}
ul.tab_bar li a {
	display: block;
*display:inline;
*zoom:1;
	padding-left: 15px;
	padding-right: 15px;
	height: 36px;
	line-height: 36px;
	text-align: center;
	-webkit-border-top-left-radius: 5px;
	-webkit-border-top-right-radius: 5px;
	-moz-border-radius-topleft: 5px;
	-moz-border-radius-topright: 5px;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
}
ul.tab_bar li a:link, ul.tab_bar li a:visited {
	color: #BA251D;
	background: none;
	text-decoration: none;
}
ul.tab_bar li a:hover, ul.tab_bar li a:active {
	color: #BA251D;
	background: #e8e8e8;
	text-decoration: underline;
}
ul.tab_bar li.cur a, ul.tab_bar li.cur a:link, ul.tab_bar li.cur a:hover, ul.tab_bar li.cur a:active, ul.tab_bar li.cur a:visited {
	color: #8b8b8b;
	background: #FFF;
	text-decoration: none;
	border: 1px solid #E2E2E2;
	border-bottom: 1px solid #FFF;
}
div.tab_content {
	min-height: 200px;
	height: auto!important;
	height: 200px;
	border: 1px solid #e2e2e2;
	background: #FFF;
	z-index: 4;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
}
div.tab_content1 {
	background: #FFF;
	z-index: 4;
	width: 100%;
	padding-top: 20px;
	padding-right: 0;
	padding-bottom: 20px;
	padding-left: 0;
}
.search_list {
	width: 100%;
	overflow: hidden;
	margin-bottom: 15px;
	margin-top: 20px;
}
.product {
	position: relative;
	width: 233px;
	margin-right: 20px;
	float: left;
}
.product .img {
	height: 170px;
	border: 1px solid #e3e3e3;
	padding: 4px;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
}
.product .title {
	height: 50px;
	line-height: 50px;
	text-align: center;
	font-size: 15px;
}
.product .price_tag {
	position: absolute;
	top: 30px;
	right: -5px;
