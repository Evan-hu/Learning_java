.mainWenku{ background: url(../images/library/wenku_bg.png) top left repeat-x; font-family: microsoft yahei}
.wenkuNav{ overflow: hidden; height: 35px; line-height: 35px;}
.wenkuNav li{ float: left; width: 118px; border-left: 1px solid #0E9769; text-align: center;}
.wenkuNav li.current{ background: url(../images/library/wenku_nav_current.png);}
.wenkuNav li a{ color: #fff; font-size: 16px; font-family: microsoft yahei;}

.wenkuTop{ overflow: hidden;}
.wenkuTop .category{ float: left; width: 237px; height: 390px; border: 1px solid #b6b6b6; border-top: 0;}
.wenkuTop .category dl{ margin: 18px 0;}
.wenkuTop .category dl dt{ height: 15px; padding-left: 15px; line-height: 15px; border-left: 6px solid #67C1A5; font-weight: bold; font-size: 14px;}
.wenkuTop .category dl dd{ margin: 10px 12px; padding: 5px 10px; border-bottom: 1px solid #e9e9e9; line-height: 20px;}
.wenkuTop .category dl.last dd{ border-bottom: 0;}
.wenkuTop .category dl.even dt{border-left: 6px solid #B3937E;}

.wenkuTop .fr{ float: right; width: 730px;}
/* 幻灯片 */
.wenkuTop .slideBox{ float: left; position:relative; width: 730px; height: 275px; margin-top: 10px; background: #f8f8f8;}
.wenkuTop .slideBox .bigImg{ display: none; height: 275px;}
.wenkuTop .slideBox .bigImg a{ display: block; height: 275px;}
.wenkuTop .slideBox .bigImg img{ position:absolute; width: 730px; height: 275px; top:0; left:0;}

/* 幻灯片数字 */
.wenkuTop .slideBox .slideUl{position:absolute; overflow: hidden; bottom: 8px; width: 730px; text-align: center;}
.wenkuTop .slideBox .slideUl li{ display:inline-block; *display:inline; *zoom:1;}
.wenkuTop .slideBox .slideUl span{ display:inline-block; *display:inline; *zoom:1; width: 10px; height:10px; margin: 0 5px; background:#ccc; border-radius:20px; line-height: 20px; cursor:pointer; color: #333; text-align: center;}
.wenkuTop .slideBox .slideUl span.on{ background: #3699DC; color: #fff;}


.wenkuTop .bannerSmall{ clear: both; height: 92px; padding-top: 12px;}
.wenkuTop .bannerSmall .item{ position: relative; float: left; width: 210px; height: 82px; padding: 5px 5px 5px 142px; border: 1px solid #e1e1e1; line-height: 24px;}
.wenkuTop .bannerSmall .item .pic{ position: absolute; left: 5px; top: 5px; width: 128px; height: 82px;}
.wenkuTop .bannerSmall .item.itemSecond {margin-left: 12px;}
.btn{ display: block; width: 83px; height: 28px; background: url(../images/library/btn_bg.jpg); line-height: 28px; text-align: center;}


.wenkuFloor{ padding-top: 20px; overflow: hidden;}
.wenkuFloor .title{ height: 40px; line-height: 40px;}
.wenkuFloor .title h2{ float: left; font-size: 20px;}
.wenkuFloor .title .typeList{ float: left; margin-left: 40px;}
.wenkuFloor .title .typeList a{ float: left; margin: 0 15px; font-size: 14px;}
.wenkuFloor .title .typeList a.current{ margin-top: 7px; line-height: 26px; color: #67C1A5; border-bottom: 2px solid #67C1A5;}
.wenkuFloor .title .typeList span.line{ float: left; color: #ccc;}


.docList{ padding: 10px; overflow: hidden;}
.docList li{ float: left; width: 120px; margin: 15px 10px;}
.docList li .pic{ display: block; width: 98px; height: 130px; margin: 0 auto;}
.docList li .name{ display: block; margin-top: 5px; line-height: 20px; text-align: center;}
.docList li p{ color: #7f7f7f; text-align: center;}

.wenkuFloorWrap{ margin-top: 20px; border-top: 1px solid #ddd;}
.wenkuFloor_1 .fl{ width: 720px;}
.wenkuFloor_1 .fr{ width: 240px;}
.wenkuFloor_1 .fr h2{ margin-bottom: 12px; line-height: 34px; font-size: 16px;} 
.wenkuFloor_1 .fr li{ position: relative; height: 24px; padding: 5px 0; font-size: 14px;}
.wenkuFloor_1 .fr li .num{ float: left; width: 16px; height: 24px; margin-right: 12px; text-align: center;}
.wenkuFloor_1 .fr li .num.numTop{ background: url(../images/library/top_1.png); text-indent: -10000px;}

.wenkuFloor_1 .fr li .total{ position: absolute; right: 0; top: 7px; font-size: 12px;}

.wenkuFloor_1 .fr .line{ height: 0; margin: 20px 0; border-bottom: 1px dotted #999;}


.wenkuFloor_2{ padding-bottom: 15px; border-top: 1px solid #ddd;}
.wenkuFloor_2 .fl{ width: 640px;}
.wenkuFloor_2 .fr{ width: 290px;}

.wenkuFloor_2 .videoList{ padding: 10px 0; overflow: hidden;}
.wenkuFloor_2 .videoList .col{ float: left; width: 285px; margin-right: 35px;}
.wenkuFloor_2 .videoList .active{ position: relative; float: left; width: 140px; height: 90px; margin-top: 10px; padding: 0px 5px 5px 142px; line-height: 24px;}
.wenkuFloor_2 .videoList .active .pic{ position: absolute; left: 0; top: 0; width: 128px; height: 82px;}
.wenkuFloor_2 .videoList .active .pic span{position: absolute; bottom: 0; left: 0; width: 100%; height: 24px; background: #000;
 opacity: 0.7; line-height: 24px; text-align: center; color: #fff;
}

.wenkuFloor_2 .videoList .col h3{font-size: 16px; line-height: 24px;}
.wenkuFloor_2 .videoList .active h3{ line-height: 20px; font-size: 14px;}
.wenkuFloor_2 .videoList .active p{ font-size: 12px; color: #7f7f7f;}
.wenkuFloor_2 .videoList  p{ line-height: 24px; font-size: 14px;}

.wenkuFloor_2 .videoList .line{height: 0; margin: 20px 0; border-bottom: 1px dotted #999;}

.wenkuFloor_2 .fr .cooper{overflow: hidden;}
.wenkuFloor_2 .fr .cooper h3{ font-size: 14px;}
.wenkuFloor_2 .fr .cooper .pic{ float: left; width: 95px; height: 95px;}
.wenkuFloor_2 .fr .cooper .info{ float: right; width: 180px; margin-top: 10px; line-height: 24px;}

.wenkuFloor_2 .fr .cooper .infoB{ clear: both; width: 287px; height: 43px; padding-top: 8px; line-height: 42px; background: url(../images/library/vip_bg.jpg); text-align: center; font-size: 14px;}
.wenkuFloor_2 .fr .cooper .infoB span{ padding: 0 8px; color: #8F6151;}

.wenkuFloor_2 .fr h2{ margin: 20px 0 10px; line-height: 30px;}
.wenkuFloor_2 .fr li{ position: relative; height: 24px; padding: 5px 0; font-size: 14px;}
.wenkuFloor_2 .fr li .num{ float: left; width: 16px; height: 24px; margin-right: 12px; text-align: center;}
.wenkuFloor_2 .fr li .num.numTop{ background: url(../images/library/top_1.png); text-indent: -10000px;}
.wenkuFloor_2 .fr li .total{ position: absolute; right: 0; top: 7px; font-size: 12px;}

.wenkuFloor_2 .fr .line{height: 0; margin: 20px 0; border-bottom: 1px dotted #999;}

.wenkuFloor_2 .fr .add{ position: relative; font-size: 14px; line-height: 24px;}
.wenkuFloor_2 .fr .add span{padding: 0 8px; color: #8F6151;}

.wenkuFloor_2 .fr .add .btn{ position: absolute; right: 0; top: 0}


.wenkuFloor_2 .typeItem{ float: left; width: 287px; margin: 12px 19px 20px; padding-bottom: 20px; border-bottom: 1px dotted #aaa;}
.wenkuFloor_2 .typeItem .pic{ width: 287px; height: 98px; overflow: hidden;}
.wenkuFloor_2 .typeItem .infoB{ clear: both; width: 287px; height: 43px; margin-top: 10px; margin-bottom: 15px; padding-top: 8px; line-height: 42px; background: url(../images/library/vip_bg.jpg); text-align: center; font-size: 14px;}
.wenkuFloor_2 .typeItem .infoB span{ padding: 0 8px; color: #8F6151;}

.wenkuFloor_2 .typeItem li{ position: relative; height: 24px; padding: 5px 0; font-size: 14px;}
.wenkuFloor_2 .typeItem li .num{ float: left; width: 16px; height: 24px; margin-right: 12px; text-align: center;}
.wenkuFloor_2 .typeItem li .num.numTop{ color: #4DA5A7; }
.wenkuFloor_2 .typeItem li .total{ position: absolute; right: 0; top: 7px; font-size: 12px;}





.categoryList{ margin-bottom: 15px; border: 1px solid #E3E8E2;}
.categoryList .title{ height: 32px; padding: 0 10px; line-height: 32px; background: #F6FCFA; border-bottom: 1px solid #E3E8E2;}
.categoryList .title h2{ float: left; font-size: 14px;}
.categoryList .title .typeList{ float: right;}
.categoryList .title .typeList a{ float: left; margin: 0 15px; font-size: 14px;}
.categoryList .title .typeList span.line{ float: left; color: #ccc;}

.categoryList .con{ padding: 15px; overflow: hidden;}

.categoryList .con .fl{ width: 185px;}
.categoryList .con .fl h4{ line-height: 24px; margin-bottom: 10px; border-bottom: 1px solid #f1f1f1; line-height: 24px;}
.categoryList .con .fl p{ line-height: 24px;}

.categoryList .con .fr{ float: right; width: 750px;}
.categoryList .con .fr .col{ float: left; width: 230px; margin: 10px;}
.categoryList .con .fr .col li{ padding: 3px 0; line-height: 24px;}
.categoryList .con .fr .col li span{ margin-right: 5px; color: #486084;}

.categoryList .con .fr .news{ margin-left: 15px; padding-top: 8px; }
.categoryList .con .fr .news li{ padding: 3px 0; border-bottom: 1px dotted #aaa;}
.categoryList .con .fr .news li a{ margin-right: 10px; color: #333; font:12px/24px Tahoma,Helvetica,Arial,'Simsun',sans-serif; }
.categoryList .con .fr .news li a span{ float: right; padding-right: 15px;}

.authWrap{height: 220px; border-bottom: 1px solid #e0e0e0; background: #fbfbfb;}
.authWrap h2{ margin: 20px 0 10px; font-size: 18px;}




.cooperAll{margin-top: 20px;}

.cooperAll h2{ margin-bottom: 10px; font-size: 18px;}
.cooperAll ul{ width: 975px; overflow: hidden; border-top: 1px solid #e3e3e3; border-left: 1px solid #e3e3e3;}
.cooperAll ul li{ float: left; width: 194px; height: 100px; border-right: 1px solid #e3e3e3; border-bottom: 1px solid #e3e3e3;}
.cooperAll ul li a{ display: block; width: 170px; height: 65px; margin: 17px auto;}
.cooperAll ul li a img{ display: block; width: 170px; height: 65px;}



.companyInfoWrap{min-height: 180px; border-bottom: 1px solid #e0e0e0; background: #fbfbfb;}
.companyInfoWrap h2{ margin: 20px 0 10px; font-size: 18px;}

.companyInfo{ margin-top: 20px;}

.companyInfo .pic{ float: left; width: 150px; height: 150px; border: 1px solid #e0e0e0;}

.companyInfo .info{ margin-left: 170px;}
.companyInfo .info h2{ line-height: 30px;}

.companyInfo .info p{line-height: 24px;}
.companyInfo .info p .num{ margin-right: 5px; font-weight: bold; font-size: 14px; color: #48917D;}
.companyInfo .info p.text{ margin-top: 10px; line-height: 18px;}


.companyWen{ margin-top: 20px; overflow: hidden;}
.companyWen .fl{ width: 710px;}

.companyWen .tab .tabSwitch{height:32px; line-height:32px; border-bottom: 2px solid #18AA79;}
.companyWen .tab .tabSwitch li{float:left; width:115px; height:32px; margin-right: 5px;}
.companyWen .tab .tabSwitch a{display:block; height:32px; text-align:center; font-size: 14px; font-family: microsoft Yahei}
.companyWen .tab .tabSwitch a:hover{background-color:#b20000; color:#fff}
.companyWen .tab .tabSwitch li.current a{background:#18AA79; color:#fff;}

.companyWen .tab .tabCon{ display: none;}
.companyWen .fl .docList li{ margin: 15px 25px}


.videoList{ overflow: hidden;}
.videoList li{ float: left; width: 160px; height: 180px; margin: 20px 38px;}
.videoList li .img{ display: block; position: relative; width: 160px; height: 90px;}
.videoList li .img b{ position: absolute; bottom: 0; left: 0; width: 100%; height: 22px; background: #000; opacity: 0.6;} 
.videoList li .img span{ position: absolute; z-index: 1; bottom: 0; left: 0; width: 155px; height: 22px; padding-right: 5px; line-height: 22px; text-align: right; color: #fff;} 
.videoList li .title{ display: block; margin: 5px 0; line-height: 18px; font-size: 14px;}
.videoList li p{ line-height: 20px; color: #6c6c6c;}
.videoList li p label{ color: #999;} 

.companyWen .fr{ width: 250px; background: #fdfdfd; border: 1px solid #E8E8E6;}
.companyWen .companyScore{ padding: 15px; border-bottom: 1px solid #E8E8E6;}
.companyWen .companyScore h2{ margin-bottom: 15px; font-size: 18px;} 
.companyWen .companyScore p{ margin-top: 15px;}

.companyWen .topList{ padding: 15px 10px;}
.companyWen .topList h2{ margin-bottom: 10px; line-height: 30px;}
.companyWen .topList li{ position: relative; height: 24px; padding: 5px 0; font-size: 14px;}
.companyWen .topList li .num{ float: left; width: 16px; height: 24px; margin-right: 12px; text-align: center;}
.companyWen .topList li .num.numTop{ background: url(../images/library/top_1.png); text-indent: -10000px;}


.wenkuDetail{ padding-top: 10px; border-top: 1px solid #ccc;}
.wenkuDetail .baseInfo{ overflow: hidden;}
.wenkuDetail .baseInfo .fl{ width: 730px;}
.wenkuDetail .baseInfo .fl h2{ margin-bottom: 10px; line-height: 30px; font-size: 22px;}
.wenkuDetail .baseInfo .fl p{ color: #7f7f7f; line-height: 24px;}

.wenkuDetail .baseInfo .fr{ width: 220px;}

.wenkuDetail .baseInfo .fr .authorInfo{ overflow: hidden;}
.wenkuDetail .baseInfo .fr .authorInfo .pic{ float: left; width: 60px; height: 60px; border: 1px solid #e0e0e0;}
.wenkuDetail .baseInfo .fr .authorInfo .pic img{ width: 60px; height: 60px;}
.wenkuDetail .baseInfo .fr .authorInfo .info{ float: right; width: 150px;}
.wenkuDetail .baseInfo .fr .authorInfo .info h2{ margin-bottom: 5px; line-height: 24px; font-weight: normal; font-size: 16px;}
.wenkuDetail .baseInfo .fr .authorInfo .info h2 img{ vertical-align: middle;} 
.wenkuDetail .baseInfo .fr .authorInfo .info p{ color: #7f7f7f;}

.wenkuDetail .baseInfo .fr .numInfo{ overflow: hidden; margin-top: 10px;}
.wenkuDetail .baseInfo .fr .numInfo li{ float: left; width: 32%; height: 36px; line-height: 18px; border-right: 1px dotted #999; text-align: center;}
.wenkuDetail .baseInfo .fr .numInfo li span{ display: block; color: #7B6357}
.wenkuDetail .baseInfo .fr .numInfo li.last{ border: 0;}

.wenkuDetail .con{ margin: 20px 0; border: 1px solid #d3d3d3; padding: 15px; font-size: 14px; line-height: 24px;}
.wenkuDetail .con .downloadLink{ display: inline-block; *display: inline; *zoom: 1; vertical-align: middle;
 width: 71px; height: 34px; margin: 15px 0; background: url(../images/mall/btn_download.jpg);}
.wenkuDetail .con .line{ width: 400px; height: 0; margin: 30px auto; border-top: 1px dashed #ddd;}
.wenkuDetail .con .faq{ display: block;  width: 150px; margin: 0 auto; color: #777; text-align: left;}


.videoAll{ margin-bottom: 30px; border: 1px solid #e5e5e5; }
.videoAll .head{ height: 42px; padding: 0 15px; background: #f8f8f8; border-bottom: 1px solid #e5e5e5; line-height: 42px;}
.videoAll .head h2{ float: left; font-size: 18px; font-family: microsoft yahei;}
.videoAll .head .more{ float: right; color: #777;}
.videoAll .hotword{ float: right; margin-right: 10px; color: #777;}
.videoAll .hotword a{ margin: 0 10px; color: #777;}
.videoAll .hotword a.current{ color: #75D29D; font-weight: bold;}

.videoAll ul{ overflow: hidden;}
.videoAll ul li{ float: left; width: 160px; height: 180px; margin: 20px 17px;}
.videoAll ul li .img{ display: block; position: relative; width: 160px; height: 90px;}
.videoAll ul li .img b{ position: absolute; bottom: 0; left: 0; width: 100%; height: 22px; background: #000; opacity: 0.6;} 
.videoAll ul li .img span{ position: absolute; z-index: 1; bottom: 0; left: 0; width: 155px; height: 22px; padding-right: 5px; line-height: 22px; text-align: right; color: #fff;} 
.videoAll ul li .title{ display: block; margin: 5px 0; line-height: 18px; font-size: 14px;}
.videoAll ul li p{ line-height: 20px; color: #6c6c6c;}
.videoAll ul li p label{ color: #999;} 

.bot{ overflow: hidden; font-size: 14px;}
.share{ float: left; }
.share label{ float: left; line-height: 32px; color: #777;}
.share img{ margin-left: 10px;}

.bot .more{ float: right;} 
.bot .more .fontOrange{ color: #FE9506;}


.comment{ margin-top: 20px; overflow: hidden;}
.comment .title{ overflow: hidden; font-family: microsoft yahei; font-size: 14px;}
.comment .title h2{ float: left; font-size: 16px;}
.comment .title .num{ float: right;}
.comment .title .num span{ margin: 0 5px; font-size: 16px; color: #FE9506;}

.comment .inputArea{ margin-top: 10px; height: 100px; line-height: 100px; border: 1px solid #e5e5e5; text-align: center;}
.comment .inputArea a{ text-decoration: underline;}
.comment .inputArea textarea{ width: 950px; margin: 8px; height: 80px; border: 0;}

.comment .submit{ margin-top: 10px; text-align: right;}
.comment .submit img{ display: inline-block; vertical-align: middle;}
.comment .submit .btnSubmit{
	display: inline-block;
	width: 100px;
	height: 30px;
	margin-left: 10px;
	border-radius: 3px;
	background: #1C7AD1;
	line-height: 30px;
	color: #fff;
	font-size: 14px;
	text-align: center;
}
.commentList{
	margin-top: 30px;
	border-top: 2px solid #e5e5e5;
}

.commentList .item{ margin-bottom: 10px; padding: 15px 5px; border-bottom: 1px solid #e5e5e5;}
.commentList .item .top { overflow: hidden;}
.commentList .item .top .title{ float: left; font-weight: bold; color: #416FAB;} 
.commentList .item .top .floorNum{ float: right; padding:0 5px; background: #d6d6d6; color: #fff} 
.commentList .item .con{ padding: 10px 0; font-size: 14px; line-height: 18px; border: 0;} 
.commentList .item .bottom{ overflow: hidden; color: #777;}
.commentList .item .bottom .time,
.commentList .item .bottom .from{  float: left; margin-right: 10px;}
.commentList .item .bottom .other{ float: right;}
.commentList .item .bottom a{ margin-right: 10px; color: #416FAB;} 
.commentList .item .bottom .other a span{ color: #FE9506;}


