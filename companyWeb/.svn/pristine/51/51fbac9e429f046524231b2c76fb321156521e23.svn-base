@Service
public class SupplierService {
  
    @Autowired
    SupplierMapper supplierMapper;
     
//  add supplier
    public boolean addSupplier(Supplier supplier){
      return this.supplierMapper.updateByPrimaryKeySelective(supplier) > 1 ? true : false;
    }
}
