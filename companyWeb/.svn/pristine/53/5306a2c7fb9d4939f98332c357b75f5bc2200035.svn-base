	padding: 0px;
	text-align: center;
	color: #FFF;
	font-weight: bold;
	background: url() no-repeat center center;
}
.product .price_tag span.n {
	font-size: 15px;
}
.product .price_tag span.t {
	font-size: 11px;
	margin-left: 2px;
}
.tab_content .product {
	margin-left: 24px;
}
.search_list {
	position: relative
}
.search_list .list1, .search_list .list2 {
