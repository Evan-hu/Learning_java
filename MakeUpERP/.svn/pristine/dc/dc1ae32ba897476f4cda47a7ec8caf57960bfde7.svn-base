package com.ga.click.dbutil;

/**
 * 产生于dbutil的异常.该异常为unchecked exception.
 * 抛出unchecked exception更有利于异常的处理,以免发生无意义的级级捕获并上抛,
 * 或者直接捕获记录日志但不处理.
 */
public class DbUtilException extends RuntimeException {
  
  private static final long serialVersionUID = 7731082627128184315L;

  public DbUtilException(Throwable root) {
    super(root);
  }

  public DbUtilException(String string, Throwable root) {
    super(string, root);
  }

  public DbUtilException(String s) {
    super(s);
  }
}
